{
	"name": "df_Descarga_Ficheros_Externos_01_CARGA_Payment_method",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_DL_Files_excel",
						"type": "DatasetReference"
					},
					"name": "CARGAPaymentMethod"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_BISTG",
						"type": "DatasetReference"
					},
					"name": "SinkCARGAPaymentMethod",
					"description": "EXT_FichadasProbadas"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          COMPANY as short,",
				"          {Forma Cobro} as string,",
				"          DESCRIPCION as string,",
				"          DS_AgrupFormaCobro as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> CARGAPaymentMethod",
				"CARGAPaymentMethod derive(DS_Agrup = left(rtrim(DS_AgrupFormaCobro),150),",
				"          ID_FormaCob = left(rtrim({Forma Cobro}),3)) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          DS_Agrup,",
				"          ID_FormaCob",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     preSQLs:['Insert into CONTROL_Log_Carga (Dw_Proceso, Dw_Descripcion, Dw_Paso, Dw_Fecha)\\nselect \\'DESCARGA_01_FICHEROSEXTERNOS\\', \\'Borrado Tabla EXT_FI_FormaCobroAgrup\\', \\'INI\\', getdate();\\n\\ntruncate table EXT_FI_FormaCobroAgrup;\\n\\nInsert into CONTROL_Log_Carga (Dw_Proceso, Dw_Descripcion, Dw_Paso, Dw_Fecha)\\nselect \\'DESCARGA_00_FICHEROSEXTERNOS\\', \\'Borrado Tabla EXT_FI_FormaCobroAgrup\\', \\'FIN\\', getdate();\\n\\nInsert into CONTROL_Log_Carga (Dw_Proceso, Dw_Descripcion, Dw_Paso, Dw_Fecha)\\nselect \\'DESCARGA_00_FICHEROSEXTERNOS\\', \\'Descarga Tabla EXT_FI_FormaCobroAgrup\\', \\'INI\\', getdate()\\n\\n'],",
				"     postSQLs:['Insert into CONTROL_Log_Carga (Dw_Proceso, Dw_Descripcion, Dw_Paso, Dw_Fecha)\\nselect \\'DESCARGA_01_FICHEROSEXTERNOS\\', \\'EXT_FI_FormaCobroAgrup\\', \\'FIN\\', getdate()'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          DS_AgrupFormaCobro = DS_Agrup,",
				"          ID_FormaCobro = ID_FormaCob",
				"     )) ~> SinkCARGAPaymentMethod"
			]
		}
	}
}