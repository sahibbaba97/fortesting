{
	"name": "df_Descarga_Ficheros_Externos_01_Ajustes_Facturas",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_DL_Files_csv_Ajustes_Facturas",
						"type": "DatasetReference"
					},
					"name": "AjustesFacturas"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_BISTG",
						"type": "DatasetReference"
					},
					"name": "SinkAjustesFacturas",
					"description": "EXT_FichadasProbadas"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Type as string,",
				"          Entity_ID as string,",
				"          Account_ID as string,",
				"          Category_ID as string,",
				"          Brand_ID as string,",
				"          {Order Type_ID} as string,",
				"          Customer_ID as string,",
				"          {Invoice date} as string,",
				"          {Order date} as string,",
				"          {Units invoiced} as string,",
				"          {Amount invoiced (Local Currency)} as string,",
				"          Currency as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> AjustesFacturas",
				"AjustesFacturas derive(DWH_DATA_UPDATE = currentDate(),",
				"          REG_TYPE = 'AJU',",
				"          Customer_ID = trim(Customer_ID)) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     preSQLs:['truncate table STG_Ajustes\\n\\n'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          Type,",
				"          Entity_ID,",
				"          Account_ID,",
				"          Category_ID,",
				"          Brand_ID,",
				"          Order_Type = {Order Type_ID},",
				"          Client_ID = Customer_ID,",
				"          Invoice_Date = {Invoice date},",
				"          Order_Date = {Order date},",
				"          Units = {Units invoiced},",
				"          Amount = {Amount invoiced (Local Currency)},",
				"          Local_Currency = Currency,",
				"          Reg_Type = REG_TYPE,",
				"          DWH_DATE_UPDATE = DWH_DATA_UPDATE",
				"     )) ~> SinkAjustesFacturas"
			]
		}
	}
}